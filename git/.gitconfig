[hub]
        protocol = https

[user]
        name = Per Magnus Veierland
        email = pveierland@gmail.com

[credential]
        helper = GIT_CREDENTIAL_HELPER

[color]
        diff = auto
        status = auto
        branch = auto
        ui = true

[core]
        excludesfile = ~/.gitignore
        editor = vim

[apply]
        whitespace = nowarn

[mergetool]
        keepBackup = false

[difftool]
        prompt = false

[help]
        autocorrect = 0

[push]
        default = simple

